{
  "sources": [
    {
      "file": "morph/data.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/nigeria-national-assembly",
        "query": "SELECT *, shorter_name AS name FROM data WHERE js_position = 'Hon' AND area != '' AND NOT name LIKE '%Anonymous%Anonymous%' ORDER BY CAST(id AS int)"
      },
      "source": "http://www.nass.gov.ng/",
      "type": "membership"
    },
    {
      "file": "manual/terms.csv",
      "type": "term"
    },
    {
      "file": "wikidata/groups.json",
      "type": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "gender-balance/results.csv",
      "type": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Nigeria/Assembly"
      }
    },
    {
      "file": "wikidata/elections.json",
      "type": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22283753"
      }
    }
  ]
}
