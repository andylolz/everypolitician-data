{
  "sources": [
    {
      "file": "morph/data.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/tonga-legislative-assembly",
        "query": "SELECT id, name, constituency, 'unknown' AS party, 2015 AS term, image, email, phone, cell, source FROM data ORDER BY id"
      },
      "source": "http://parliament.gov.to/",
      "type": "membership"
    },
    {
      "file": "morph/wikidata-members.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/tonga-wikidata-members",
        "query": "SELECT item as id, itemlabel as name, itemaltlabel as other_name, date(start) as start_date, date(end) AS end_date, email, 2015 AS term, constituencylabel as constituency FROM data ORDER BY id"
      },
      "type": "membership"
    },
    {
      "file": "manual/terms.csv",
      "type": "term"
    },
    {
      "file": "gender-balance/results.csv",
      "type": "gender",
      "create": {
        "from": "gender-balance",
        "source": "Tonga/Assembly"
      }
    },
    {
      "file": "wikidata/elections.json",
      "type": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22696370"
      }
    }
  ]
}
