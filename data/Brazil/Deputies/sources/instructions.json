{
  "sources": [
    {
      "file": "morph/official.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/brasil-deputados-historic",
        "query": "SELECT *, NULL as birth_date, NULL as death_date FROM data d JOIN membership m ON d.id = m.id WHERE term IN (54, 55) ORDER BY id, term DESC"
      },
      "source": "http://www.camara.gov.br",
      "type": "membership"
    },
    {
      "file": "morph/official-55.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/brasil-deputados-scraper",
        "query": "SELECT id, name, fullname, phone, image, email, source FROM data WHERE term = 55 ORDER BY id"
      },
      "source": "http://www.camara.gov.br",
      "type": "person",
      "merge": {
        "incoming_field": "name",
        "existing_field": "name",
        "reconciliation_file": "reconciliation/official-55.csv"
      }
    },
    {
      "file": "morph/wikidata.csv",
      "create": {
        "from": "morph",
        "scraper": "tmtmtmtm/brasil-deputados-wikidata",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "source": "http://wikidata.org/",
      "type": "wikidata",
      "merge": {
        "incoming_field": "name",
        "existing_field": "fullname",
        "reconciliation_file": "reconciliation/wikidata.csv"
      }
    },
    {
      "file": "manual/terms.csv",
      "type": "term"
    },
    {
      "file": "morph/areas.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/brazil-states",
        "query": "SELECT * FROM data ORDER BY id"
      },
      "type": "area-wikidata",
      "merge": {
        "reconciliation_file": "reconciliation/areas.csv"
      }
    },
    {
      "file": "morph/genderbalance.csv",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/brazil-deputies-gender-balance",
        "query": "SELECT uuid, gender FROM data ORDER BY uuid"
      },
      "source": "http://gender-balance.org/",
      "type": "person",
      "merge": {
        "incoming_field": "uuid",
        "existing_field": "uuid"
      }
    },
    {
      "file": "morph/cabinet.csv",
      "type": "wikidata-cabinet",
      "create": {
        "from": "morph",
        "scraper": "everypolitician-scrapers/brazil-positions",
        "query": "SELECT * FROM data ORDER BY id, position, start_date"
      }
    },
    {
      "file": "wikidata/groups.json",
      "type": "group",
      "create": {
        "from": "group-wikidata",
        "source": "manual/group_wikidata.csv"
      }
    },
    {
      "file": "wikidata/elections.json",
      "type": "wikidata-elections",
      "create": {
        "from": "election-wikidata",
        "base": "Q22338585"
      }
    }
  ]
}
